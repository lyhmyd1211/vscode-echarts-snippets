{
  "echarts-line-charts": {
    "prefix": "ecLine",
    "body": [
      "option = {",
      "\txAxis: {",
      "\t\ttype: 'category',",
      "\t\tdata: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']",
      "\t},",
      "\tyAxis: {",
      "\t\ttype: 'value'",
      "\t},",
      "\tseries: [{",
      "\t\tdata: [820, 932, 901, 934, 1290, 1330, 1320],",
      "\t\ttype: 'line',",
      "\t}]",
      "}"
    ],
    "description": "echarts折线图基础配置\necharts basic line chart"
  },
  "echarts-line-area-charts": {
    "prefix": "ecLineArea",
    "body": [
      "option = {",
      "\txAxis: {",
      "\t\ttype: 'category',",
      "\t\tdata: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']",
      "\t},",
      "\tyAxis: {",
      "\t\ttype: 'value'",
      "\t},",
      "\tseries: [{",
      "\t\tdata: [820, 932, 901, 934, 1290, 1330, 1320],",
      "\t\ttype: 'line',",
      "\t\tareaStyle: {}",
      "\t}]",
      "}"
    ],
    "description": "echarts折线区域图基础配置\necharts basic line area chart"
  },
  "echarts-bar-charts": {
    "prefix": "ecBar",
    "body": [
      "option = {",
      "\tcolor: ['#3398DB'],",
      "\ttooltip: {",
      "\t\ttrigger: 'axis',",
      "\t\taxisPointer: {            // 坐标轴指示器，坐标轴触发有效",
      "\t\t\ttype: 'shadow'        // 默认为直线，可选为：'line' | 'shadow'",
      "\t\t}",
      "\t},",
      "\tgrid: {",
      "\t\tleft: '3%',",
      "\t\tright: '4%',",
      "\t\tbottom: '3%',",
      "\t\tcontainLabel: true",
      "\t},",
      "\txAxis: [",
      "\t\t{",
      "\t\t\ttype: 'category',",
      "\t\t\tdata: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],",
      "\t\t\taxisTick: {",
      "\t\t\t\talignWithLabel: true",
      "\t\t\t}",
      "\t\t}",
      "\t],",
      "\tyAxis: [",
      "\t\t{",
      "\t\t\ttype: 'value'",
      "\t\t}",
      "\t],",
      "\tseries: [",
      "\t\t{",
      "\t\t\tname: '直接访问',",
      "\t\t\ttype: 'bar',",
      "\t\t\tbarWidth: '60%',",
      "\t\t\tdata: [10, 52, 200, 334, 390, 330, 220]",
      "\t\t}",
      "\t]",
      "};"
    ],
    "description": "echarts柱状图基础配置\necharts basic bar chart"
  },

  "echarts-pie-charts": {
    "prefix": "ecPie",
    "body": [
      "option = {",
      "\ttooltip: {",
      "\t\t  trigger: 'item',",
      "\t\tformatter: '{a} <br/>{b}: {c} ({d}%)'",
      "\t},",
      "\tlegend: {",
      "\t\t  orient: 'vertical',",
      "\t\tleft: 10,",
      "\t\tdata: ['直接访问', '邮件营销', '联盟广告', '视频广告', '搜索引擎'],",
      "\tseries: [",
      "\t\t {",
      "\t\t\t name: '访问来源',",
      "\t\t\ttype: 'pie',",
      "\t\t\tradius: ['50%', '70%'],",
      "\t\t\tavoidLabelOverlap: false,",
      "\t\t\tlabel: {",
      "\t\t\t\tnormal: {",
      "\t\t\t\t\tshow: false,",
      "\t\t\t\t\tposition: 'center'",
      "\t\t\t},",
      "\t\t\temphasis: {",
      "\t\t\t\tshow: true,",
      "\t\t\t\ttextStyle: {",
      "\t\t\t\t\tfontSize: '30',",
      "\t\t\t\t\tfontWeight: 'bold'",
      "\t\t\t\t}",
      "\t\t\t}",
      "\t\t\t},",
      "\t\t\tlabelLine: {",
      "\t\t\t\tnormal: {",
      "\t\t\t\t\tshow: false",
      "\t\t\t\t}",
      "\t\t\t},",
      "\t\t\tdata: [",
      "\t\t\t\t{value: 335, name: '直接访问'},",
      "\t\t\t\t{value: 310, name: '邮件营销'},",
      "\t\t\t\t{value: 135, name: '视频广告'},",
      "\t\t\t\t{value: 234, name: '联盟广告'},",
      "\t\t\t\t {value: 1548, name: '搜索引擎'}",
      "\t\t\t]",
      "\t\t}",
      "\t]",
      "\t}",
      "}"
    ],
    "description": "echarts饼图基础配置\n echarts basic pie chart"
  },
  "echarts-radar-charts": {
    "prefix": "ecRadar",
    "body": [
      "option = {",
      "  title: {",
      "    text: '基础雷达图'",
      "  },",
      "  tooltip: {},",
      "  legend: {",
      "    data: ['预算分配（Allocated Budget）', '实际开销（Actual Spending）']",
      "  },",
      "  radar: {",
      "    // shape: 'circle',",
      "    name: {",
      "      textStyle: {",
      "        color: '#fff',",
      "        backgroundColor: '#999',",
      "        borderRadius: 3,",
      "        padding: [3, 5]",
      "      }",
      "    },",
      "    indicator: [",
      "      { name: '销售（sales）', max: 6500 },",
      "      { name: '管理（Administration）', max: 16000 },",
      "      { name: '信息技术（Information Techology）', max: 30000 },",
      "      { name: '客服（Customer Support）', max: 38000 },",
      "      { name: '研发（Development）', max: 52000 },",
      "      { name: '市场（Marketing）', max: 25000 }",
      "    ]",
      "  },",
      "  series: [",
      "    {",
      "      name: '预算 vs 开销（Budget vs spending）',",
      "      type: 'radar',",
      "      // areaStyle: {normal: {}},",
      "      data: [",
      "        {",
      "          value: [4300, 10000, 28000, 35000, 50000, 19000],",
      "          name: '预算分配（Allocated Budget）'",
      "        },",
      "        {",
      "          value: [5000, 14000, 28000, 31000, 42000, 21000],",
      "          name: '实际开销（Actual Spending）'",
      "        }",
      "      ]",
      "    }",
      "  ]",
      "}"
    ],
    "description": "echarts雷达图基础配置\n echarts basic radar chart"
  },
  "echarts-gauge-charts": {
    "prefix": "ecGauge",
    "body": [
      "option = {",
      "  tooltip: {",
      "    formatter: '{a} <br/>{b} : {c}%'",
      "  },",
      "  toolbox: {",
      "    feature: {",
      "      restore: {},",
      "      saveAsImage: {}",
      "    }",
      "  },",
      "  series: [",
      "    {",
      "      name: '业务指标',",
      "      type: 'gauge',",
      "      detail: { formatter: '{value}%' },",
      "      data: [{ value: 50, name: '完成率' }]",
      "    }",
      "  ]",
      "}"
    ],
    "description": "echarts仪表盘基础配置\n echarts basic gauge chart"
  }
}
